	borderRadius: 5,
	borderWidth: '1dp',
	borderColor: "#e4e4e4"
},
"#actualTimeLbl":{
	width: '100%',
	height: '100%',
	backgroundColor: '#fff',
	font: {
		fontSize: '22dp',
		fontWeight: 'bold'
	},
	color: '#333',
	textAlign: Ti.UI.TEXT_ALIGNMENT_CENTER,
}
