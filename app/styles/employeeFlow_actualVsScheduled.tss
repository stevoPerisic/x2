"#actualVsScheduled":{
	backgroundColor:"#fcfcfc",
	fullscreen: true,
	navBarHidden: true
},
"#content":{
	top: '8%',
	backgroundColor: '#fcfcfc',
	width: Ti.UI.FILL
},
"#actualVsScheduledLbl":{
	top: '20dp',
	font: {
		fontSize: '30dp'
	}
},
"#chooseAShift":{
	top: '120dp',
	width: '100%',
	height: '600dp',
	left: 0,
	layout: 'vertical'
},
